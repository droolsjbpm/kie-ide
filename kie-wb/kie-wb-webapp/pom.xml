              </configuration>
            </plugin>
          </plugins>
        </pluginManagement>
      </build>
    </profile>

    <!-- Full profile builds all GWT permutions -->
    <profile>
      <id>fullProfile</id>
      <activation>
        <property>
          <name>full</name>
        </property>
      </activation>
      <build>
        <pluginManagement>
          <plugins>
            <plugin>
              <groupId>org.codehaus.mojo</groupId>
              <artifactId>gwt-maven-plugin</artifactId>
              <configuration>
                <module>org.kie.workbench.KIEWebapp</module>
                <draftCompile>false</draftCompile>
                <localWorkers>4</localWorkers>
                </configuration>
              </plugin>
            </plugins>
          </pluginManagement>
        </build>
      </profile>

  </profiles>

</project>
