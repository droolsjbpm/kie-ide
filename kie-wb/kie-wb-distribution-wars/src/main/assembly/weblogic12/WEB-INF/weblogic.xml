<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-web-app" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://xmlns.oracle.com/weblogic/weblogic-web-app http://xmlns.oracle.com/weblogic/weblogic-web-app/1.4/weblogic-web-app.xsd">
  <wls:weblogic-version>12.1.3</wls:weblogic-version>
  <wls:container-descriptor>
    <wls:prefer-application-packages>
      <wls:package-name>com.google.common.*</wls:package-name>
      <wls:package-name>org.xmlpull.*</wls:package-name>
      <wls:package-name>org.apache.commons.io.*</wls:package-name>
      <wls:package-name>org.joda.*</wls:package-name>
      <wls:package-name>org.codehaus.jackson.*</wls:package-name>
    </wls:prefer-application-packages>
  </wls:container-descriptor>
  <wls:security-role-assignment>
    <wls:role-name>admin</wls:role-name>
    <wls:principal-name>admin</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>user</wls:role-name>
    <wls:principal-name>user</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>developer</wls:role-name>
    <wls:principal-name>developer</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>manager</wls:role-name>
    <wls:principal-name>manager</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>analyst</wls:role-name>
    <wls:principal-name>analyst</wls:principal-name>
  </wls:security-role-assignment>
  <!-- rest security -->
  <wls:security-role-assignment>
    <wls:role-name>rest-all</wls:role-name>
    <wls:principal-name>rest-all</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-project</wls:role-name>
    <wls:principal-name>rest-project</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-deployment</wls:role-name>
    <wls:principal-name>rest-deployment</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-process</wls:role-name>
    <wls:principal-name>rest-process</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-process-read-only</wls:role-name>
    <wls:principal-name>rest-process-read-only</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-task</wls:role-name>
    <wls:principal-name>rest-task</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-task-read-only</wls:role-name>
    <wls:principal-name>rest-task-read-only</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-query</wls:role-name>
    <wls:principal-name>rest-query</wls:principal-name>
  </wls:security-role-assignment>
  <wls:security-role-assignment>
    <wls:role-name>rest-client</wls:role-name>
    <wls:principal-name>rest-client</wls:principal-name>
  </wls:security-role-assignment>
</wls:weblogic-web-app>